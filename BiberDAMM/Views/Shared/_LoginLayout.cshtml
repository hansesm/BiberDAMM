@using WebGrease.Css.ImageAssemblyAnalysis
<!--This is the Layout for the Login Page [KrabsJ]-->
<!--Customized by [HansesM]-->

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>DAMM-@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="container body-content">

        <img src="/Content/Images/LogoDamm.png" alt="Damm Logo" width="400" />

        @RenderBody()

        <hr />

        <footer>
            <p>&copy; @DateTime.Now.Year - Biber Consulting</p>
        </footer>

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

